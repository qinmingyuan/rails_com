<%= form_with model: @acme_order, url: { action: 'update' } do |f| %>
  <%= render 'shared/error_messages', target: @acme_order %>
  <% @acme_order.identifiers.each do |identifier| %>
    <%= render partial: 'item_field', locals: { f: f, index: 0, value: identifier } %>
  <% end %>
  <%= f.submit %>
<% end %>
